{
  "name": "desia",
  "version": "1.0.0",
  "description": "DESIA_JewelrySite",
  "engines": {
    "node": "12.13.0",
    "npm": "6.13.7"
  },
  "main": "app.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 1 --ignore 'scss' --ignore 'static' ",
    "dev:assets": "cd src && WEBPACK_ENV=development  webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production  webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static build && cp -R src/views build && cp -R src/resources build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "tunnel": "lt --port 4000",
    "start": "node build/init.js",
    "prestart": "npm run build"
  },
  "author": "sooss",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "7.8.3",
    "@babel/core": "^7.5.4",
    "@babel/node": "7.7.7",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.7.7",
    "autoprefixer": "9.7.4",
    "aws-sdk": "2.610.0",
    "axios": "0.19.2",
    "babel-loader": "^8.0.6",
    "body-parser": "1.19.0",
    "connect-mongo": "3.2.0",
    "cookie-parser": "1.4.4",
    "coolsms-node-sdk": "0.0.2",
    "coolsms-sdk-v4": "4.0.15",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "helmet": "3.21.2",
    "moment": "2.24.0",
    "mongoose": "5.8.7",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "multer-s3": "2.9.0",
    "node-sass": "4.13.0",
    "nodemailer": "6.4.2",
    "nodemailer-smtp-transport": "2.7.4",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "6.0.1",
    "passport-naver": "1.0.6",
    "populate": "5.0.1",
    "postcss-loader": "3.0.0",
    "pug": "2.0.4",
