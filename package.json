    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "8.0.6",
    "cross-env": "6.0.3",
    "jest": "25.1.0",
    "mkdirp": "1.0.3",
    "nodemon": "2.0.2",
    "regenerator-runtime": "0.13.3"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
